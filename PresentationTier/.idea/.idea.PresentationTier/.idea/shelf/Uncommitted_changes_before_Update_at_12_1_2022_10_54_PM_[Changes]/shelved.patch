 password = \"\";\r\n    string fName = \"\";\r\n    string lName = \"\";\r\n    string msg = \"\";\r\n\r\n    private async Task Create()\r\n    {\r\n        msg = \"\";\r\n\r\n        if (userName == \"\" || password == \"\" || fName == \"\" || lName == \"\")\r\n        {\r\n            msg = \"All fields are required\";\r\n            return;\r\n        }\r\n\r\n        try\r\n        {\r\n            var dto = new UserCreationDTO\r\n            {\r\n                userName = userName,\r\n                password = password,\r\n                FirstName = fName,\r\n                LastName = lName\r\n            };\r\n\r\n            await UserService.CreateAsync(dto);\r\n            NavManager.NavigateTo(\"/Login\");\r\n        }\r\n        catch (Exception e)\r\n        {\r\n            Console.WriteLine(e);\r\n            msg = e.Message;\r\n        }\r\n    }\r\n}
